@model IEnumerable<MusicStore.Models.Genre>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<h3>Browse Genres</h3>
<p>
    Select from @Model.Count()
    genres:
</p>
<ul>
    <ul>
        @foreach (var genre in Model)
        {
            <li>@Html.ActionLink(genre.Name, "Browse", new { genre = genre.Name })</li>
        }
    </ul>
</ul>
